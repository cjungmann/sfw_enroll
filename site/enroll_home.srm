$database            : SFW_Enroll
$xml-stylesheet      : default.xsl
$default-mode        : home

$session-type        : identity
$test_authorized     : App_Session_Confirm
$jump-not-authorized : Login.srm

home
   procedure : App_Home_Page
   type   : form-page
   schema
   button
      label : Profile
      type : open
      url  : profile.srm
   button
      label : Logout
      type : jump
      url  : login.srm?logout
